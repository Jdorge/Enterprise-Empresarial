{
  "_info": {
    "created": "2025-08-12",
    "version": "2.0.0",
    "encrypted": false,
    "note": "IMPORTANTE: Nunca commitar este arquivo com dados reais. Use variáveis de ambiente em produção."
  },
  "apis": {
    "openai": {
      "api_key": "${OPENAI_API_KEY}",
      "organization": "${OPENAI_ORG_ID}"
    },
    "anthropic": {
      "api_key": "${ANTHROPIC_API_KEY}"
    },
    "google": {
      "api_key": "${GOOGLE_API_KEY}",
      "project_id": "${GOOGLE_PROJECT_ID}"
    },
    "notion": {
      "token": "${NOTION_TOKEN}",
      "database_id": "${NOTION_DATABASE_ID}"
    },
    "xai": {
      "api_key": "${XAI_API_KEY}"
    }
  },
  "services": {
    "firebase": {
      "project_id": "${FIREBASE_PROJECT_ID}",
      "private_key": "${FIREBASE_PRIVATE_KEY}",
      "client_email": "${FIREBASE_CLIENT_EMAIL}"
    },
    "github": {
      "token": "${GITHUB_TOKEN}",
      "username": "${GITHUB_USERNAME}"
    }
  },
  "encryption": {
    "enabled": false,
    "key_file": "encryption.key",
    "algorithm": "AES-256"
  }
}
